{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "shared-node-browser": true,
    "mocha": true,
    "phantomjs": true,
    "amd": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-no-bind": 0,
    "react/require-default-props": 0,
    "react/no-danger": 0,
    "max-len": [1, 120, 2]
  }
}
